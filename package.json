{
  "name": "flux-pricing",
  "version": "0.0.1",
  "description": "Pricing component with flux",
  "main": "js/app.js",
  "dependencies": {
    "flux": "^2.0.0",
    "jest": "^14.0.1",
    "keymirror": "^0.1.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-test-utils": "0.0.1",
    "react-tools": "^0.13.3",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "browserify": "~13.1.0",
    "envify": "~3.4.1",
    "react": "^15.3.0",
    "reactify": "^1.1",
    "watchify": "~3.7.0"
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d .",
    "build": "browserify . | uglifyjs -cm > js/bundle.min.js",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "author": "Ken Wheeler",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
